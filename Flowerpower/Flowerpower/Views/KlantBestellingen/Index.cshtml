@model IEnumerable<Flowerpower.Models.bestelling>

@{
    
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.winkelmand.FirstOrDefault().producten.productnaam)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.bestellinggeplaatst)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.klant.naam)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.medewerkers.medewerkernaam)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.winkel.winkelnaam)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.winkelmand.FirstOrDefault().producten.productnaam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.bestellinggeplaatst)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.klant.naam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.medewerkers.medewerkernaam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.winkel.winkelnaam)
            </td>
            
        </tr>
    }
    
    </table>
</body>
</html>
